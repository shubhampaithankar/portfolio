{
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "react/no-unescaped-entities": "off",
    "@typescript-eslint/quotes": "off",
    "quotes": [0],
    "avoidEscape": 0,
    "allowemplateLiterals": 0,
    "no-useless-escape": 0
  }
}
